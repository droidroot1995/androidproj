        </activity>
        <activity
            android:name=".player1"
            android:label="@string/title_activity_player1" >
        </activity>
        <activity
            android:name=".playerX"
            android:label="@string/title_activity_player_x" >
        </activity>
        <activity
            android:name=".player0"
            android:label="@string/title_activity_player0" >
        </activity>
        <activity
            android:name=".players"
            android:label="@string/title_activity_players" >
        </activity>
    </application>

</manifest>
